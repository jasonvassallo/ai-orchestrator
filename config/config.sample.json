{
  "$schema": "./config-schema.json",
  "version": "2.0.0",
  "_comment": "\u26a0\ufe0f SECURITY: NEVER store API keys in this file! Use the credential manager instead.",
  "defaults": {
    "preferLocal": false,
    "localProvider": "mlx",
    "costOptimize": false,
    "enableLLMRouting": true,
    "routerAllTasks": true,
    "routingModel": "gemini-3-flash-preview",
    "preferSubscriptionProviders": [
      "vertex-ai"
    ],
    "preferSubscriptionModels": [
      "vertex-gemini-3-pro"
    ],
    "maxTokens": 4096,
    "temperature": 0.7,
    "timeout": 120
  },
  "models": {
    "claude-opus-4.5": {
      "enabled": true,
      "priority": 90
    },
    "claude-sonnet-4.5": {
      "enabled": true,
      "priority": 80
    },
    "gpt-4o": {
      "enabled": true,
      "priority": 75
    },
    "gpt-5.2": {
      "enabled": true,
      "priority": 85
    },
    "o1": {
      "enabled": true,
      "priority": 85
    },
    "gemini-2.0-flash": {
      "enabled": true,
      "priority": 70
    },
    "mlx-llama-vision-11b": {
      "enabled": true,
      "priority": 60
    }
  },
  "providers": {
    "openai": {
      "organization": "",
      "baseUrl": "https://api.openai.com/v1"
    },
    "anthropic": {
      "baseUrl": "https://api.anthropic.com"
    },
    "vertex-ai": {
      "location": "global"
    }
  },
  "rateLimits": {
    "openai": {
      "requestsPerMinute": 60,
      "tokensPerMinute": 150000
    },
    "anthropic": {
      "requestsPerMinute": 60,
      "tokensPerMinute": 100000
    },
    "google": {
      "requestsPerMinute": 60,
      "tokensPerMinute": 120000
    }
  },
  "logging": {
    "level": "INFO",
    "redactSensitive": true
  },
  "security": {
    "maxPromptLength": 500000,
    "maxMessages": 100,
    "allowRemoteExecution": false
  },
  "taskRouting": {
    "general": [
      "mlx-qwen3-4b",
      "vertex-gemini-3-flash",
      "vertex-gemini-3-pro"
    ],
    "code": [
      "mlx-qwen2.5-coder-14b",
      "kimi-k2-thinking",
      "vertex-gemini-3-pro"
    ],
    "reasoning": [
      "kimi-k2-thinking",
      "vertex-gemini-3-pro"
    ],
    "math": [
      "kimi-k2-thinking",
      "vertex-gemini-3-pro"
    ],
    "data-analysis": [
      "vertex-gemini-3-pro"
    ],
    "multimodal": [
      "vertex-gemini-3-pro",
      "mlx-llama-vision-11b"
    ],
    "long-context": [
      "vertex-gemini-3-pro"
    ],
    "websearch": [
      "perplexity-sonar-pro",
      "perplexity-sonar-reasoning-pro"
    ],
    "creative": [
      "vertex-gemini-3-pro",
      "claude-opus-4.5"
    ],
    "summarization": [
      "claude-haiku-4.5",
      "vertex-gemini-3-flash"
    ],
    "local": [
      "mlx-qwen3-4b",
      "mlx-qwen2.5-coder-14b",
      "mlx-llama-vision-11b",
      "mlx-ministral-14b-reasoning"
    ]
  },
  "musicGeneration": {
    "_comment": "MusicGen AI model for audio generation (requires .music-venv)",
    "defaultModel": "musicgen-small",
    "availableModels": [
      "musicgen-small",
      "musicgen-medium",
      "musicgen-large",
      "musicgen-stereo-small",
      "musicgen-stereo-medium",
      "musicgen-stereo-large",
      "musicgen-melody",
      "musicgen-melody-large"
    ]
  }
}
